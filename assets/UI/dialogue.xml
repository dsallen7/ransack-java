					</children>
				</value>
				<value class="org.mini2Dx.ui.element.Row" id="okButtonRow">
					<visibility>VISIBLE</visibility>
					<children length="1">
						<value class="org.mini2Dx.ui.element.TextButton" id="dialogueOkButton">
							<visibility>VISIBLE</visibility>
							<children length="1">
								<value class="org.mini2Dx.ui.element.Label">
									<visibility>VISIBLE</visibility>
									<text>Ok</text>
									<horizontalAlignment>CENTER</horizontalAlignment>
								</value>
							</children>
						</value>
					</children>
				</value>
			</children>
